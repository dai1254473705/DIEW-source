@import 'components/wux/wux.wxss';

/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: left;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page{font-family: "Microsoft YaHei";color:#333;}
view{box-sizing: border-box;}
.clearfix:before,
.clearfix:after {
  content: "";
  display: table;
}
.clearfix:after {
  clear: both;
}
.clearfix {
  clear: both;
  /**zoom: 1;*/
}

